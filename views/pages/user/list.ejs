<!DOCTYPE html>
<html>
<head>
    
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Page Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="main.css" />
    <script src="main.js"></script>
</head>
<body>

    <table>
        <thead>
            <td>Nama Gejala/ Penyakit</td>
            <td>Deskripsi</td>
            <td>Nama Obat</td>
            <td>Harga</td>
            <td>Nama Brand</td>
        </thead>

        <tbody>
            <% for (let data of list) { %>
                <tr>
                    <td><%= data.name %></td>
                    <td><%= data.description %></td>
                    <td><%= data.drugName %></td>
                    <td><%= data.price %></td>
                    <td><%= data.brandName %></td>
                    <td><a href="/login/edit/<%= data.id %>">Edit</a> | <a href="/login/delete/<%= data.id %>" onclick="return confirm('Are you sure to delete <%= data.name %>?')">Delete</a> </td>
                </tr>
                <% } %>
        </tbody>
    </table>
    
</body>
</html>